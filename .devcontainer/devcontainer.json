{
  "name": "Azure Docker Nap",
  "dockerComposeFile": [
    "../.docker/docker-compose.yml",
    "../.docker/docker-compose.debug.yml"],
  "service": "azure_docker_nap",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python@2024.22.0",
        "ms-python.debugpy@2024.14.0",
        "ms-python.vscode-pylance@2024.12.1",
        "streetsidesoftware.code-spell-checker@4.0.21",
        "ms-azuretools.vscode-bicep@0.32.4"]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers/features/azure-cli:1": {
      "installBicep": true 
    }
  }
}